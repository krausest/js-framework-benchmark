  "private": true,
  "type": "module",
  "dependencies": {
    "jstat": "^1.9.6",
    "plotly.js-cartesian-dist": "2.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.1"
  },
  "scripts": {
    "start": "webpack serve --mode development -c webpack.config.dev.js",
    "build": "webpack --mode production",
    "lint": "eslint src/",
