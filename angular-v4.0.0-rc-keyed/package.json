{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for angular 2",
  "scripts": {
    "start": "npm run build-dev && npm run serve",
    "test": "node_modules/.bin/karma start",
    "precompile": "node_modules/.bin/ngc",
    "rollup-aot": "node_modules/.bin/rollup -c rollup.config.aot.js",
    "rollup-jit": "node_modules/.bin/rollup -c rollup.config.jit.js",
    "build-prod": "npm run precompile && npm run rollup-aot && npm run rollup-jit",
    "build-dev": "npm run build-prod"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~4.0.0-rc",
    "@angular/compiler": "~4.0.0-rc",
    "@angular/compiler-cli": "~4.0.0-rc",
    "@angular/core": "~4.0.0-rc",
    "@angular/platform-browser": "~4.0.0-rc",
    "@angular/platform-browser-dynamic": "~4.0.0-rc",
    "@angular/platform-server": "~4.0.0-rc",
    "core-js": "2.4.1",
    "rxjs": "5.0.3",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "google-closure-compiler-js": "20170124.0.0",
    "http-server": "0.9.0",
    "rollup": "0.41.4",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-uglify": "1.0.1",
    "typescript": "2.2.1",
    "typings": "2.1.0",
    "yargs": "7.0.1"
  }
}
