{
	"name": "js-framework-benchmark-literaljs",
	"version": "1.0.0",
	"description": "Benchmark for LiteralJS framework",
	"js-framework-benchmark": {
		"frameworkVersionFromPackage": "literaljs",
		"customURL": "/dist"
	},
	"main": "index.js",
	"scripts": {
		"start": "parcel index.html",
		"dev": "parcel index.html --open",
		"build-prod": "cross-env NODE_ENV=production parcel build index.html --public-url /frameworks/non-keyed/literaljs/dist/",
		"test": "echo \"Error: no test specified\" && exit 1",
		"rebuild": "sudo rm -rf node_modules && rm -rf yarn.lock && yarn && yarn dev"
	},
	"keywords": [
		"literaljs"
	],
	"author": "Michael Farrell",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "7.8.4",
		"literaljs": "7.0.0"
	},
	"devDependencies": {
		"@babel/core": "7.8.6",
		"@babel/plugin-transform-react-jsx": "7.8.3",
		"@babel/plugin-transform-runtime": "7.8.3",
		"cross-env": "7.0.1",
		"node-sass": "4.13.1",
		"parcel-bundler": "1.12.4"
	}
}
